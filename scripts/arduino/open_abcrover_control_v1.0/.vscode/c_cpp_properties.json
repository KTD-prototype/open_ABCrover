{
    "configurations": [
        {
            "name": "Linux",
            "defines": [
                "ARDUINO=10800"
            ],
            "includePath": [
                "${workspaceRoot}",
                "/opt/arduino-1.8.12/hardware/arduino/avr/**",
                "/opt/arduino-1.8.12/tools/**",
                "/opt/arduino-1.8.12/hardware/tools/**",
                "/opt/arduino-1.8.12/libraries/**",
                "/home/ktd/Arduino/hardware/tools/**",
                "/home/ktd/Arduino/libraries/**"
            ],
            "browse":{
                "path":[
                    "/opt/arduino-1.8.12/libraries",
                    "/home/ktd/Arduino/libraries"
                ]
            },
            "forcedInclude": [
                "/opt/arduino-1.8.12/hardware/arduino/avr/cores/arduino/Arduino.h"
            ],
            "intelliSenseMode": "clang-x64",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "compilerPath": "/usr/bin/clang"
        }
    ],
    "version": 4
}